<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import org.tbee.javafx.scene.layout.fxml.MigPane?>
<!--<?import layouttests.migfxml.sample1.*?>-->

<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.FlowPane?>
<MigPane id="rootPane" fx:controller="LoginController"
         styleClass="main-panel"
         layout="insets 20"
         cols="[label, pref!][grow, 50::]"
         rows=""
         xmlns:fx="http://javafx.com/fxml">

    <FlowPane orientation="VERTICAL">
        <TextField fx:id="login"></TextField>
        <PasswordField fx:id="password"></PasswordField>
        <Button onAction="#login" text="Login"/>
        <Label fx:id="message"/>
    </FlowPane>
</MigPane >